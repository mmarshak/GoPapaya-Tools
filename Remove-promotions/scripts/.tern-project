{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "./*.js"
  ],
   "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}